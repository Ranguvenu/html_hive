{{#response}}
    {{#viewtypeCard}}
    <div class="" id="sessions_tab_content">
        <table class="w-full generaltable" id = sessionsdata>
            <thead>
                <tr>
                    <th>{{#str}}sessionid,local_classroom{{/str}}</th>
                    <th>{{#str}}name{{/str}}</th>
                    <th>{{#str}}date{{/str}}</th>
                    <th>{{#str}}time{{/str}}</th>
                    <th>{{#str}}status,local_classroom{{/str}}</th>
                    <th>{{#str}}attended_sessions_users,local_classroom{{/str}}</th>
                    <th>{{#str}}faculty,local_classroom{{/str}}</th>
                </tr>
            </thead>
            {{#records}}
        <tr>
            <td >{{id}} </td>
            <td >{{name}}</td>
            <td class="generatable_data"><i class="fa fa-calendar" aria-hidden="true"></i> <span class="calender_date">{{date}}</span></td>
            <td><i class="fa fa-clock-o"></i>{{starttime}} <b>-</b>{{endtime}}</td>
            <td>{{status}}</td>
            <td>{{attendacecount}}</td>
            <td>{{{trainer}}}</td>
        </tr>
        {{/records}}
        </table>
    </div>
    {{/viewtypeCard}}
{{/response}}
