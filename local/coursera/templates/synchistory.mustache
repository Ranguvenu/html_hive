{{#response}}
    {{#viewtypeCard}}
        <table id ="display_synchistory" class="generaltable">
        <thead>
            <tr>
                <th>Type</th>
                <th>Data</th>
                <th>Response</th>
                <th>Status</th>
                <th>Excuted By</th>
                <th>Excuted Date And Time</th>
