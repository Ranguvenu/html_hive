                    <td>{{moduletype}}</td>
                    <td>{{modules}}</td>
                    <td>{{statusmessage}}</td>
                    <td>{{{status}}}</td>
                    <td>{{usercreated}}</td>
                    <td>{{timecreated}}</td>
                </tr>
            {{/records}}
        </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}
