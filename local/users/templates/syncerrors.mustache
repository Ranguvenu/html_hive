{{#response}}
    {{#viewtypeCard}}
        <table id ="displaysyncerrors" class="generaltable">
        <thead>
            <tr>
                <th>Employee ID</th>
                <th>Email</th>
                <th>Error with</th>
                <th>Description</th>
                <th>Uploaded by</th>
                <th>Uploaded On</th>
            </tr>
        </thead>
        <tbody>
            {{#records}}
                <tr>
                    <td>{{idnumber}}</td>
                    <td>{{email}}</td>
                    <td>{{{mandatoryfields}}}</td>
                    
                    <td>{{{errors}}}</td>
                    <td>{{modifiedby}}</td>
                    <td>{{createddate}}</td>
                </tr>
            {{/records}}
        </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}
